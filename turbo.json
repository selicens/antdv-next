{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build:token": {
      "cache": false,
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "antdv-next#build:token"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^build", "antdv-next#build:token"],
      "cache": false,
      "persistent": true
    }
  }
}
