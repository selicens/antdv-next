{
  "name": "@antdv-next/tailwind",
  "type": "module",
  "version": "1.0.0-beta.0",
  "description": "Tailwind CSS plugin for Ant Design Vue Next",
  "repository": {
    "type": "git",
    "url": "https://git.lingyu.org.cn/antdv-next/ui.git"
  },
  "keywords": [
    "tailwindcss",
    "tailwind-plugin",
    "tailwindcss-v4",
    "ant-design",
    "antdv",
    "vue",
    "design-system"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./v4": {
      "types": "./dist/v4.d.mts",
      "import": "./dist/v4.mjs",
      "default": "./dist/v4.mjs"
    },
    "./theme.css": "./theme.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "README.md",
    "README.zh-CN.md",
    "dist",
    "theme.css"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublish": "pnpm build",
    "bump": "bumpp --commit \"chore(release): @antdv-next/tailwind %s\" --push --tag \"@antdv-next/tailwind@%s\""
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "tailwindcss": "catalog:dev"
  }
}
